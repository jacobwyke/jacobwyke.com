{
    "name": "jacobwyke",
    "version": "1.0.0",
    "description": "jacobwyke.com Website",
    "main": "webpack.config.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "version": "webpack --version",
        "dev": "concurrently \"npm run dev:jekyll\" \"npm run dev:webpack\"",
        "dev:jekyll": "bundle exec jekyll build -s src/www/ -d build/_jekyll --watch --force_polling",
        "dev:webpack": "NODE_ENV=development webpack --watch",
        "test": "npm run test:lint",
        "test:lint": "eslint --ignore-pattern=build/ .",
        "jekyll": "bundle exec jekyll build -s src/www/ -d build/_jekyll",
        "build": "npm run build:dev",
        "build:dev": "npm run jekyll; NODE_ENV=development webpack",
        "build:prod": "npm run jekyll; NODE_ENV=production webpack -p",
        "build:prod:debug": "npm run jekyll; DEBUG_MODE=true NODE_ENV=production webpack -p",
        "start": "npm run dev"
    },
    "keywords": [],
    "author": "Jacob Wyke",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/jacobwyke/jacobwyke.git"
    },
    "license": "UNLICENSED",
    "dependencies": {},
    "devDependencies": {
        "autoprefixer": "^8.0.0",
        "babel-core": "^6.26.0",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-plugin-transform-vue-jsx": "^3.5.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-2": "^6.24.1",
        "chalk": "^2.3.1",
        "concurrently": "^3.5.1",
        "copy-webpack-plugin": "^4.4.1",
        "css-loader": "^0.28.10",
        "eslint": "^4.19.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.9",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "html-loader": "^0.5.5",
        "html-minifier": "^3.5.14",
        "html-webpack-plugin": "^2.30.1",
        "imagemin-webpack-plugin": "^2.1.1",
        "node-notifier": "^5.2.1",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "ora": "^2.0.0",
        "portfinder": "^1.0.13",
        "postcss-import": "^11.1.0",
        "postcss-loader": "^2.1.0",
        "postcss-url": "^7.3.2",
        "rimraf": "^2.6.2",
        "semver": "^5.5.0",
        "shelljs": "^0.8.1",
        "style-loader": "^0.20.2",
        "uglifyjs-webpack-plugin": "^1.2.2",
        "url-loader": "^0.6.2",
        "webpack": "^3.11.0",
        "webpack-bundle-analyzer": "^2.10.0",
        "webpack-merge": "^4.1.2",
        "webpack-plugin-hash-output": "^1.3.0"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    }
}
